{
  "name": "office-addin-taskpane",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-TaskPane.git"
  },
  "license": "MIT",
  "config": {
    "app_to_debug": "word",
    "app_type_to_debug": "desktop"
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "sideload": "npx office-addin-dev-settings sideload dist/manifest.xml",
    "start:dev": "concurrently \"npm:dev-server\" \"npm:sideload\"",
    "start": "office-addin-debugging start manifest.xml",
    "stop": "office-addin-debugging stop manifest.xml",
    "stop:dev": "npm run unsideload",
    "unsideload": "npx office-addin-dev-settings uninstall dist/manifest.xml"
  },
  "dependencies": {
    "@types/shortid": "^2.2.0",
    "core-js": "^3.36.0",
    "fast-json-patch": "^3.1.1",
    "fast-xml-parser": "^5.2.3",
    "regenerator-runtime": "^0.14.1",
    "shortid": "^2.2.17",
    "saxon-js": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@types/office-js": "^1.0.493",
    "@types/office-runtime": "^1.0.35",
    "babel-loader": "^9.1.3",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^12.0.2",
    "eslint-plugin-office-addins": "^4.0.3",
    "file-loader": "^6.2.0",
    "find-free-port": "^2.0.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "kill-port": "^2.0.1",
    "office-addin-cli": "^2.0.3",
    "office-addin-debugging": "^6.0.3",
    "office-addin-dev-certs": "^2.0.3",
    "office-addin-lint": "^3.0.3",
    "office-addin-manifest": "^2.0.3",
    "office-addin-prettier-config": "^2.0.1",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "source-map-loader": "^5.0.0",
    "stream-browserify": "^3.0.0",
    "typescript": "^5.4.2",
    "url": "^0.11.4",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "5.1.0"
  },
  "prettier": "office-addin-prettier-config",
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ]
}
